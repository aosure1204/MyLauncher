            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/five"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="日历"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="960px"
            android:layout_height="504px"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/six"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="设置"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="960px"
            android:layout_height="504px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/serven"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="图库"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="960px"
            android:layout_height="504px"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/eight"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="音乐"/>
        </LinearLayout>
    </com.wedesign.launcher3.CellLayout>

    <com.wedesign.launcher3.CellLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="960px"
            android:layout_height="504px"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/nine"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="QQ输入法"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="960px"
            android:layout_height="504px"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ten"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Android键盘"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="960px"
            android:layout_height="504px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/eleven"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="计算器"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="960px"
            android:layout_height="504px"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/twelve"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="货车导航"/>
        </LinearLayout>
    </com.wedesign.launcher3.CellLayout>

</com.wedesign.launcher3.Workspace>